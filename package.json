  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "chalk": "^4.1.2",
    "eslint": "^8.55.0",
    "inquirer": "^9.2.12",
    "jest": "^29.7.0",
    "json-diff": "^1.0.6",
